# Example environment file
# Copy this file to .env and fill in your actual credentials

# Telegram API Configuration
# Get these from https://my.telegram.org
TELEGRAM_API_ID=YOUR_API_ID_HERE
TELEGRAM_API_HASH=YOUR_API_HASH_HERE
TELEGRAM_PHONE=+1234567890
TELEGRAM_CHANNEL=t.me/yourchannel

# Facebook API Configuration
# Get these from https://developers.facebook.com
FACEBOOK_PAGE_TOKEN=YOUR_FACEBOOK_PAGE_TOKEN_HERE
FACEBOOK_PAGE_ID=YOUR_FACEBOOK_PAGE_ID_HERE

# AliExpress API Configuration (Optional)
# Get these from https://portals.aliexpress.com
ALIEXPRESS_APP_KEY=YOUR_APP_KEY
ALIEXPRESS_SECRET_KEY=YOUR_SECRET_KEY
USE_ALIEXPRESS=True

# Bot Settings
COPY_EXACT_TEXT=True
ADD_AFFILIATE_LINKS=False
